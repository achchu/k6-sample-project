{
  "name": "k6-performance-testing",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev:api": "ts-node --transpile-only src/api/server.ts",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "test:perf:simple": "k6 run performance/k6-test.ts",
    "test:perf:load:simple": "k6 run performance/k6-load-test.ts",
    "test:perf": "k6 run --out influxdb=http://myorg:some-auth-token@localhost:8086/k6db performance/k6-test.ts",
    "test:perf:load": "k6 run --out influxdb=http://myorg:some-auth-token@localhost:8086/k6db performance/k6-load-test.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/axios": "^0.9.36",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^22.13.0",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "k6": "^0.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.14",
    "@types/k6": "^0.54.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
